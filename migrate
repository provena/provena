# :command.initialize
initialize() {
  version="0.1.0"
  long_usage=''
  set -e

  # src/initialize.sh

}

# :command.run
run() {
  declare -A args=()
  declare -A deps=()
  declare -a other_args=()
  declare -a env_var_names=()
  declare -a input=()
  normalize_input "$@"
  parse_requirements "${input[@]}"

  case "$action" in
    "history-migration") migrate_history_migration_command ;;
    "f1339-modelrun-display-name-migration") migrate_f1339_modelrun_display_name_migration_command ;;
    "f1249-migration") migrate_f1249_migration_command ;;
    "f1249-f1247-migration") migrate_f1249_f1247_migration_command ;;
    "f1176-migration") migrate_f1176_migration_command ;;
    "f1442-migration") migrate_f1442_migration_command ;;
    "f1458-migration") migrate_f1458_migration_command ;;
    "clear-app-stage") migrate_clear_app_stage_command ;;
    "f1545-migration") migrate_f1545_migration_command ;;
    "f1580-migration") migrate_f1580_migration_command ;;
    "f1699-migration") migrate_f1699_migration_command ;;
  esac
}

initialize
run "$@"
