# :command.usage
migrate_usage() {
  if [[ -n $long_usage ]]; then
    printf "migrate - This CLI tool provides a set of migrations which are shell wrappers of automated migration processes.\n"
    echo

  else
    printf "migrate - This CLI tool provides a set of migrations which are shell wrappers of automated migration processes.\n"
    echo

  fi

  printf "%s\n" "Usage:"
  printf "  migrate COMMAND\n"
  printf "  migrate [COMMAND] --help | -h\n"
  printf "  migrate --version | -v\n"
  echo
  # :command.usage_commands
  printf "%s\n" "Commands:"
  printf "  %s   Peforms the RRAPIS-1247 migration - updating registry items to include the starting history entry.\n" "history-migration                    "
  printf "  %s   Performs model run display name migration\n" "f1339-modelrun-display-name-migration"
  printf "  %s   Migrations required for f1249 feature (dataset ethics/export/indigenous + person ethics) - defaults to unapproved ethics\n" "f1249-migration                      "
  printf "  %s   Migrations required for f1249 + f1247 features (history + dataset ethics/export/indigenous + person ethics) - defaults to unapproved ethics\n" "f1249-f1247-migration                "
  printf "  %s   Migrations required for f1176 feature - adds owner_username, renames author -> associations, changes publisher/org to IDs, removes rocrate_metadata field - only runs on PROD - no migration defined for other stages - just clear them.\n" "f1176-migration                      "
  printf "  %s   Migrations required for f1442 feature - adds the versioning info version = 1 for Entity category items\n" "f1442-migration                      "
  printf "  %s   Migrations required for f1458 feature - adds the versioning info version = 1 for Entity category items\n" "f1458-migration                      "
  printf "  %s   CAUTION: Clears all items from the registry and graph store for a given stage. This does not allow PROD by default. You will need to manually clear PROD. This is a migration used when other forms of migration are too time intensive for test items (usually when manual intervention required).\n" "clear-app-stage                      "
  printf "  %s   Migrations required for f1545 feature - Access Info URI in non-reposited datasets for fetch\n" "f1545-migration                      "
  printf "  %s   Migrations required for f1580 feature - moves the software_version field from MRWT into model run(s) which use it\n" "f1580-migration                      "
  printf "  %s   Migrations required for f1699 feature - Creates a subfield/dict for the now optional creation and published dates in dataset items.\n" "f1699-migration                      "
  echo

  # :command.long_usage
  if [[ -n $long_usage ]]; then
    printf "%s\n" "Options:"

    # :command.usage_fixed_flags
    printf "  %s\n" "--help, -h"
    printf "    Show this help\n"
    echo
    printf "  %s\n" "--version, -v"
    printf "    Show version number\n"
    echo

  fi
}

