# :command.parse_requirements
parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --version | -v)
        version_command
        exit
        ;;

      --help | -h)
        long_usage=yes
        migrate_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action=${1:-}

  case $action in
    -*) ;;

    history-migration)
      action="history-migration"
      shift
      migrate_history_migration_parse_requirements "$@"
      shift $#
      ;;

    f1339-modelrun-display-name-migration)
      action="f1339-modelrun-display-name-migration"
      shift
      migrate_f1339_modelrun_display_name_migration_parse_requirements "$@"
      shift $#
      ;;

    f1249-migration)
      action="f1249-migration"
      shift
      migrate_f1249_migration_parse_requirements "$@"
      shift $#
      ;;

    f1249-f1247-migration)
      action="f1249-f1247-migration"
      shift
      migrate_f1249_f1247_migration_parse_requirements "$@"
      shift $#
      ;;

    f1176-migration)
      action="f1176-migration"
      shift
      migrate_f1176_migration_parse_requirements "$@"
      shift $#
      ;;

    f1442-migration)
      action="f1442-migration"
      shift
      migrate_f1442_migration_parse_requirements "$@"
      shift $#
      ;;

    f1458-migration)
      action="f1458-migration"
      shift
      migrate_f1458_migration_parse_requirements "$@"
      shift $#
      ;;

    clear-app-stage)
      action="clear-app-stage"
      shift
      migrate_clear_app_stage_parse_requirements "$@"
      shift $#
      ;;

    f1545-migration)
      action="f1545-migration"
      shift
      migrate_f1545_migration_parse_requirements "$@"
      shift $#
      ;;

    f1580-migration)
      action="f1580-migration"
      shift
      migrate_f1580_migration_parse_requirements "$@"
      shift $#
      ;;

    f1699-migration)
      action="f1699-migration"
      shift
      migrate_f1699_migration_parse_requirements "$@"
      shift $#
      ;;

    # :command.command_fallback
    "")
      migrate_usage >&2
      exit 1
      ;;

    *)
      printf "invalid command: %s\n" "$action" >&2
      exit 1
      ;;

  esac

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

}

# :command.parse_requirements
migrate_history_migration_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --help | -h)
        long_usage=yes
        migrate_history_migration_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="history-migration"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --apply)

        # :flag.case_no_arg
        args['--apply']=1
        shift
        ;;

      # :flag.case
      --reuse_venv)

        # :flag.case_no_arg
        args['--reuse_venv']=1
        shift
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        if [[ -z ${args['stage']+x} ]]; then

          args['stage']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['stage']+x} ]]; then
    printf "missing required argument: STAGE\nusage: migrate history-migration STAGE [OPTIONS]\n" >&2
    exit 1
  fi

  # :command.whitelist_filter
  if [[ -n ${args['stage']:-} ]] && [[ ! ${args['stage']:-} =~ ^(TEST|DEV|STAGE|PROD)$ ]]; then
    printf "%s\n" "stage must be one of: TEST, DEV, STAGE, PROD" >&2
    exit 1
  fi

}

# :command.parse_requirements
migrate_f1339_modelrun_display_name_migration_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --help | -h)
        long_usage=yes
        migrate_f1339_modelrun_display_name_migration_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="f1339-modelrun-display-name-migration"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --apply)

        # :flag.case_no_arg
        args['--apply']=1
        shift
        ;;

      # :flag.case
      --reuse_venv)

        # :flag.case_no_arg
        args['--reuse_venv']=1
        shift
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        if [[ -z ${args['stage']+x} ]]; then

          args['stage']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['stage']+x} ]]; then
    printf "missing required argument: STAGE\nusage: migrate f1339-modelrun-display-name-migration STAGE [OPTIONS]\n" >&2
    exit 1
  fi

  # :command.whitelist_filter
  if [[ -n ${args['stage']:-} ]] && [[ ! ${args['stage']:-} =~ ^(TEST|DEV|STAGE|PROD)$ ]]; then
    printf "%s\n" "stage must be one of: TEST, DEV, STAGE, PROD" >&2
    exit 1
  fi

}

