{
    "deployment": {
        "config_id": "DEV",
        "stage": "DEV",
        "config_source_repo_clone_string": "https://github.com/your-org/your-config-repo.git",
        "config_source_name_space": "provena",
        "config_source_stage": "dev",
        "config_repo_oauth_token_secret_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
        "pipeline_stack_id": "DEVPipeline",
        "deployment_stack_id": "DEV-provena",
        "git_owner_org": "your-org",
        "git_repo_name": "provena",
        "git_branch_name": "main",
        "deployment_environment": {
            "account": "AWS ACCOUNT ID",
            "region": "AWS REGION (recommended ap-southeast-2)"
        },
        "pipeline_environment": {
            "account": "AWS ACCOUNT ID",
            "region": "AWS REGION (recommended ap-southeast-2)"
        },
        "github_token_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
        "main_pipeline_trigger": "WEBHOOK",
        "cdk_out_path": "dev_cdk.out",
        "sentry_config": {
            "monitoring_enabled": false
        },
        "interface_pipeline": true,
        "quick_deploy_pipeline": true,
        "interface_pipeline_trigger_settings": "NONE",
        "build_badge": {
            "build_domain": "deploy-build-badge.dev",
            "interface_domain": "interface-build-badge.dev"
        },
        "ui_only_pipeline": true,
        "ui_pipeline_trigger_settings": "NONE",
        "email_alerts_activated": true,
        "pipeline_alert_email": "alerts@email.address.com",
        "cross_account": false,
        "ci_hook_pr_into_branch": false,
        "trusted_ui_build_account_ids": null,
        "git_commit_id": "${GIT_COMMIT_ID?'!!'||null}",
        "git_commit_url": "${GIT_COMMIT_URL?'!!'||null}",
        "git_tag_name": "${GIT_TAG_NAME?'!!'||null}",
        "git_release_title": "${GIT_RELEASE_TITLE?'!!'||null}",
        "git_release_url": "${GIT_RELEASE_URL?'!!'||null}"
    },
    "components": {
        "networking": {},
        "keycloak": {
            "domain": "auth.dev",
            "realm_name": "provena",
            "snapshot_arn": "arn:aws:rds:YOUR_SNAPSHOT_ARN"
        },
        "identity_service": {
            "handle_credentials_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "domain_name": "handle.dev",
            "ardc_service_endpoint": "https://demo.identifiers.ardc.edu.au/pids"
        },
        "data_store": {
            "service_account_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "oidc_service_account_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "api_domain": "data-api.dev",
            "ui_domain": "data.dev"
        },
        "landing_page": {
            "ui_domain": "dev"
        },
        "auth_api": {
            "api_domain": "auth-api.dev",
            "api_service_account_secret_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "access_alerts_email_address": "alerts@email.address.com"
        },
        "search": {
            "search_service_domain": "search-service.dev",
            "api_domain": "search.dev"
        },
        "entity_registry": {
            "service_account_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "api_domain": "registry-api.dev",
            "ui_domain": "registry.dev"
        },
        "prov_store": {
            "api_domain": "prov-api.dev",
            "ui_domain": "prov.dev",
            "neo4j_http_domain": "prov-db.dev",
            "neo4j_bolt_domain": "prov-db-bolt.dev",
            "neo4j_auth_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "neo4j_memory_size": 4096,
            "neo4j_cpu_size": 1024,
            "service_account_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
            "neo4j_service_instance_role_arn": "arn:aws:iam:YOUR_INSTANCE_ROLE_ARN",
            "prov_job_dispatcher_service_role_secret_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN"
        },
        "warmer": {
            "domain": "warmer.dev"
        },
        "async_jobs": {
            "job_api_domain": "job-api.dev"
        }
    },
    "tests": {
        "test_activation": {
            "TS_TYPE_CHECK": true,
            "MYPY_TYPE_CHECK": true,
            "UNIT": true,
            "INTEGRATION": true,
            "SYSTEM": false
        },
        "unit_tests": {
            "handle_secret_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN"
        },
        "integration_tests": {
            "keycloak_client_id": "integration-tests",
            "integration_test_bots_creds_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN"
        }
    },
    "general": {
        "email_connection_secret_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
        "dockerhub_creds_arn": "arn:aws:secretsmanager:YOUR_SECRET_ARN",
        "storage_bucket_arn": "arn:aws:s3:YOUR_BUCKET_ARN",
        "application_root_domain": "dev.your.domain.com",
        "documentation_base_link": "https://docs.provena.io",
        "contact_us_link": "https://your-support-link.com",
        "rate_limiting": {
            "throttling_burst_limit": 20,
            "throttling_rate_limit": 10
        },
        "tags": {
            "application": "Provena",
            "author": "Provena",
            "stage": "DEV",
            "branch": "main"
        }
    },
    "dns": {
        "root_domain": "your.domain.com",
        "hosted_zone_name": "your.domain.com",
        "hosted_zone_id": "YOUR_HOSTED_ZONE_ID",
        "domain_certificate_arn": "arn:aws:acm:app-region:YOUR_DOMAIN_CERT_ARN",
        "us_east_certificate_arn": "arn:aws:acm:us-east-1:YOUR_US_EAST_CERT_ARN"
    },
    "backup": {
        "backups_enabled": true,
        "vault_name": "YOUR_BACKUP_VAULT",
        "key_admins": [
            "arn:aws:iam::TRUSTED_ROLE_ARN_1",
            "arn:aws:iam::TRUSTED_ROLE_ARN_2"
        ]
    }
